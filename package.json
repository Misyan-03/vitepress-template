  "release-it": {
    "github": {
      "release": true,
      "releaseName": "Release v${version}"
    },
    "git": {
      "push": true,
      "tagName": "v${version}",
      "commitMessage": "release: v${version}",
      "requireUpstream": false
    },
    "npm": {
      "publish": true
    },
    "hooks": {
      "after:bump": "npm run build",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    },
